# Create .gitignore file
cat > .gitignore << 'EOF'
# Dependencies
node_modules/
venv/
env/
.venv/
*.pyc
__pycache__/

# Build outputs
dist/
build/
.next/
out/
*.egg-info/

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Testing
coverage/
.nyc_output/
.pytest_cache/

# Terraform
*.tfstate
*.tfstate.*
.terraform/
.terraform.lock.hcl

# AWS
.aws/

# Docker
docker-compose.override.yml

# Temporary files
tmp/
temp/
*.tmp

# Secrets (extra safety)
secrets/
*.pem
*.key
*.crt

# OS
Thumbs.db
EOF